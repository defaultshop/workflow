    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/vue-fontawesome": "^0.1.8",
    "@panter/vue-i18next": "^0.15.1",
    "@processmaker/screen-builder": "0.23.1",
    "@processmaker/vue-form-elements": "0.14.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.4",
    "bpmn-moddle": "^6.0.0",
    "i18next": "^15.0.8",
    "jointjs": "^3.1.1",
    "lodash": "^4.17.15",
    "luxon": "^1.21.1",
    "vue": "^2.6.10",
    "vue-color": "^2.7.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 2%",
    "last 2 versions",
    "not ie <= 8",
    "chrome >= 69"
  ],
  "nyc": {
    "extension": [
      ".js",
      ".vue"
    ],
    "exclude": [
      "tests/**/*.js"
    ],
    "report-dir": "cypress-coverage"
  }
}
